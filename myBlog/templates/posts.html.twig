{% extends 'base.html.twig' %}

{% block content %}

    <ul>
    {% for post in posts %}

        <li>
            <!--<a href="/posts/{{ post.id }}">-->
            <a href="{{ path('post', {id: post.id}) }}">
                {{ post.title }} -
                {{ post.date|date('d/m/Y') }}
            </a>
        </li>


    {% endfor %}
    </ul>


{% endblock %}

